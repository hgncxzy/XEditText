<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <com.xzy.xedittext.edittext.zhifubao.WidgetEditText
        android:id="@+id/et_input"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/dp_10_5"
        android:gravity="center"
        app:icv_et_bg_complete="@drawable/shape_icv_et_bg_complete"
        app:icv_et_bg_focus="@drawable/shape_icv_et_bg_press"
        app:icv_et_bg_normal="@drawable/shape_icv_et_bg_normal"
        app:icv_et_divider_drawable="@drawable/shape_divider_identifying"
        app:icv_et_number="6"
        app:icv_et_text_color="#4A4A4A"
        app:icv_et_text_size="@dimen/dp_20"
        app:icv_et_width="@dimen/dp_40" />
    <Button
        android:id="@+id/btn_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_60"
        android:text="跳转"
        tools:ignore="HardcodedText" />
</LinearLayout>